{
	"name": "DB Plain Interpreter",
	"version": "1.0",
	"timezone": "Europe/Berlin",
	"constraints": [
		{
			"key": "ricsCode",
			"values": ["0080", "1080"]
		}, 
		{
			"key": "messageTypeVersion",
			"values": ["1"]
		},
		{
			"key":"recordVersion",
			"values": ["01"]
		},
		{
			"key":"layoutStandard",
			"values": ["PLAI"]
		}
	],
	"elements": [
		{
			"type":"productName",
			"fields": [
				{
					"line":1,
					"column":1
				}
			]
		},
		{
			"type":"validFrom",
			"delimiter":".",
			"format":"dd.MM.yyyy HH:mm",
			"fields": [
				{
					"line":2,
					"column":1,
					"substring": [4, 16]
				}
			]
		},
		{
			"type":"validUntil",
			"delimiter":".",
			"format":"dd.MM.yyyy HH:mm",
			"fields": [
				{
					"line":2,
					"column":1,
					"substring": [25, 16]
				}
			]
		},
		{
			"type":"passengerName",
			"fields": [
				{
					"line":8,
					"column":1
				}
			]
		},
		{
			"type":"infoText",
			"fields": [
				{
					"line":4,
					"column":1
				}
			]
		}
	]
}